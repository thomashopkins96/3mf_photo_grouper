    "build:client": "vite build client",
    "start": "node server/dist/index.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.160.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.6"
  }
}
